                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/et_View_country2"/>

            <TextView
                android:id="@+id/et_View_state2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                android:padding="4dp"
                android:text="Tamil Nadu "
                android:textColor="#0A0606"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_View_state2" />

            <TextView
                android:id="@+id/tv_View_city2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="City "
                android:textStyle="bold"
                android:padding="4dp"
                android:background="@color/white"
                android:layout_margin="8dp"
                android:textColor="#0A0606"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/et_View_state2"/>

            <TextView
                android:id="@+id/et_View_city2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:layout_marginTop="8dp"
                android:background="@color/white"
                android:padding="4dp"
                android:text=" "
                android:textColor="#0A0606"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_View_city2" />

            <TextView
