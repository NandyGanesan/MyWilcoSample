                android:padding="4dp"
                android:text="Address"
                android:textStyle="bold"
                android:textColor="#0A0606"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
